{
  "type": "InputService",
  "version": 1,
  "inputs": [
    {
      "id": 0,
      "name": "Simulated Inputs",
      "type": "uint64_t"
    }
  ],
  "outputs": [
    {
      "id": 0,
      "name": "Active Inputs",
      "type": "uint64_t"
    },
    {
      "id": 1,
      "name": "Input Event",
      "type": "uint8_t[2]"
    }
  ],
  "registers": [
    {
      "id": 0,
      "name": "Input Configs",
      "type": "blob"
    },
    {
      "id": 1,
      "name": "Debounce Time",
      "type": "uint32_t",
      "default": 10
    },
    {
      "id": 2,
      "name": "Long Press Time",
      "type": "uint32_t",
      "default": 500
    },
    {
      "id": 3,
      "name": "Lift Multiple Delay",
      "type": "uint32_t",
      "default": 10
    }
  ],
  "enums": [
    {
      "id": "InputEventType",
      "base_type": "uint8_t",
      "values": {
        "ACTIVE": 0,
        "INACTIVE": 1,
        "SHORT": 2,
        "LONG": 3
      }
    }
  ]
}
